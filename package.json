{
  "name": "frontend-template",
  "version": "0.0.1",
  "description": "Starter kit for modern web applications",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/victorsoares96/frontend-template.git"
  },
  "bugs": {
    "url": "https://github.com/victorsoares96/frontend-template/issues",
    "email": "vitorsoares96@hotmail.com"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc --skipLibCheck && vite build --debug",
    "preview": "vite preview",
    "https-preview": "serve dist --ssl --cors --port 3002",
    "prepare": "husky install",
    "style:check": "prettier --check .",
    "style:fix": "prettier --write .",
    "lint:check": "eslint ./src/ --ext .ts,.js,.json",
    "lint:fix": "eslint --fix ./src/ --ext .ts,.js,.json",
    "type-check": "tsc -p tsconfig.json --noEmit --pretty --skipLibCheck"
  },
  "dependencies": {
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@fontsource/montserrat": "4.5.12",
    "@fontsource/poppins": "4.5.9",
    "@fontsource/work-sans": "4.5.12",
    "@mui/icons-material": "5.10.6",
    "@mui/lab": "5.0.0-alpha.101",
    "@mui/material": "5.10.7",
    "@mui/system": "5.10.7",
    "@mui/utils": "5.10.6",
    "@reduxjs/toolkit": "1.8.5",
    "axios": "0.27.2",
    "formik": "2.2.9",
    "i18next": "21.9.2",
    "i18next-browser-languagedetector": "6.1.5",
    "i18next-http-backend": "1.4.4",
    "i18next-xhr-backend": "3.2.2",
    "is-mobile": "3.1.1",
    "lottie-react": "2.3.1",
    "notistack": "2.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-helmet-async": "1.3.0",
    "react-hotkeys-hook": "3.4.7",
    "react-i18next": "11.18.6",
    "react-icons": "4.4.0",
    "react-redux": "8.0.4",
    "react-router-dom": "6.4.1",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "3.3.0",
    "@types/node": "17.0.45",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "@vitejs/plugin-react": "2.1.0",
    "commitizen": "4.2.5",
    "commitizen-emoji": "1.0.5",
    "eslint": "8.24.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "https-localhost": "4.7.1",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "msw": "0.49.3",
    "prettier": "2.7.1",
    "typescript": "4.8.4",
    "vite": "3.1.4",
    "vite-plugin-pwa": "0.13.1"
  },
  "author": {
    "name": "Victor Soares",
    "email": "vitorsoares96@hotmail.com"
  },
  "keywords": [
    "react",
    "pwa",
    "starter-kit",
    "vite"
  ],
  "engines": {
    "npm": "please use yarn",
    "yarn": ">= 1.17.3",
    "node": ">= 16.14.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/commitizen-emoji"
    },
    "commitizenEmoji": {
      "conventionalFormat": true
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
