{
  "name": "frontend-template",
  "version": "1.0.0",
  "description": "Starter kit for modern web applications",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/victorsoares96/frontend-template.git"
  },
  "bugs": {
    "url": "https://github.com/victorsoares96/frontend-template/issues",
    "email": "vitorsoares96@hotmail.com"
  },
