    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "https-localhost": "^4.7.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.2",
    "msw": "^0.47.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.2",
    "vite": "^2.9.9",
    "vite-plugin-pwa": "^0.12.0"
