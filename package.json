    "@mui/material": "^5.10.4",
    "@mui/system": "^5.8.1",
    "@mui/utils": "^5.8.0",
    "@reduxjs/toolkit": "^1.8.5",
    "axios": "^0.27.2",
    "formik": "^2.2.9",
    "i18next": "^21.9.1",
    "i18next-browser-languagedetector": "^6.1.5",
    "i18next-http-backend": "^1.4.1",
